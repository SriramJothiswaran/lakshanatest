                                               ng-disabled="seatLayout[33].reserved || result.length == parseInt(maxSeats) "
                                               type="checkbox" value="None"
                                               id="asiento34" name="check"/>
                                        <label ng-class="{'selected' : seatLayout[33].reserved == false && disable[33].value == true , 'notselected' : seatLayout[33].reserved == true, 'disabled-class' : result.length == intChange && disable[33].value == false && seatLayout[33].reserved == false }"
