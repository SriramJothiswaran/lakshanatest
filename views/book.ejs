                                               for="asiento34">{{seatLayout[33].id}}</label>
                                    </div>
                                </div>
                                <div class="seccion">
                                    <div class="asiento">
                                        <input ng-checked="seatLayout[34].reserved"
                                               ng-disabled="seatLayout[34].reserved || result.length == parseInt(maxSeats) "
                                               type="checkbox" value="None"
                                               id="asiento35" name="check"/>
                                        <label ng-class="{'selected' : seatLayout[34].reserved == false && disable[34].value == true , 'notselected' : seatLayout[34].reserved == true, 'disabled-class' : result.length == intChange && disable[34].value == false && seatLayout[34].reserved == false }"
                                               ng-click="checkDisabled(seatLayout[34].reserved,result.length,maxSeats,seatLayout[34])"
